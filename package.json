{
  "name": "gitlab-radiator",
  "author": {
    "name": "heikkipora",
    "email": "heikki.pora@gmail.com"
  },
  "description": "The missing GitLab build radiator view",
  "version": "2.4.1",
  "license": "MIT",
  "bin": {
    "gitlab-radiator": "bin/gitlab-radiator"
  },
  "scripts": {
    "build": "./build-npm",
    "start": "NODE_TLS_REJECT_UNAUTHORIZED=0 babel-node src/app.js",
    "eslint": "eslint --fix src/* bin/* test/*",
    "test": "mocha --require @babel/register --require @babel/polyfill --timeout 20000 test/*.js"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heikkipora/gitlab-radiator.git"
  },
  "bugs": {
    "url": "https://github.com/heikkipora/gitlab-radiator/issues"
  },
  "homepage": "https://github.com/heikkipora/gitlab-radiator#readme",
  "contributors": [
    {
      "name": "codegeneralist",
      "email": "antti.oittinen@gmail.com"
    }
  ],
  "dependencies": {
    "@babel/runtime": "7.3.1",
    "axios": "0.18.0",
    "basic-auth": "2.0.1",
    "compression": "1.7.3",
    "date-fns": "1.30.1",
    "esm": "^3.2.4",
    "express": "4.16.4",
    "js-yaml": "3.12.1",
    "lodash": "4.17.11"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "7.3.2",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/polyfill": "7.2.5",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-eslint": "10.0.1",
    "babelify": "10.0.0",
    "browserify": "16.2.3",
    "browserify-middleware": "8.1.1",
    "chai": "4.2.0",
    "eslint": "5.13.0",
    "eslint-plugin-mocha": "5.2.1",
    "eslint-plugin-react": "7.12.4",
    "less-middleware": "3.1.0",
    "mocha": "5.2.0",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "socket.io": "2.2.0",
    "uglify-js": "3.4.9"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "@babel/env",
            "@babel/react"
          ],
          "plugins": [
            "@babel/plugin-proposal-class-properties"
          ]
        }
      ]
    ]
  }
}
